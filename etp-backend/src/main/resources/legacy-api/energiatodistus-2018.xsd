												<xsd:element name="IvErillispoistotPoisto" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Ilmanvaihdon erillispoiston
															poistoilmavirta (m3/s).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="IvErillispoistotSfp" minOccurs="0" maxOccurs="1">
													<xsd:annotation>
														<xsd:documentation>Ilmvaihdon erillispoiston
															ominaissähköteho (kW/(m3*s)).</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="vakiot:PosDecimalType">
															<xsd:maxInclusive value="10.0"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="IvIvjarjestelmaTulo" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Ilmanvaihtojärjestelmän tuloilmavirta
															(m3/s).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="IvIvjarjestelmaPoisto" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Ilmanvaihtojärjestelmän poistoilmavirta
															(m3/s).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="IvIvjarjestelmaSfp" minOccurs="0" maxOccurs="1">
													<xsd:annotation>
														<xsd:documentation>Ilmanvaihtojärjestelmän
															ominaissähköteho (kW/(m3*s)).</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="vakiot:PosDecimalType">
															<xsd:maxInclusive value="10.0"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="IvLtoVuosihyotysuhde" minOccurs="0" maxOccurs="1">
													<xsd:annotation>
														<xsd:documentation>Rakennuksen ilmanvaihtojärjestelmän
															LTO:n vuosihyötysuhde</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="vakiot:PosDecimalType">
															<xsd:maxInclusive value="1"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="LahtotiedotLammitys" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="LammitysKuvaus_Fi" minOccurs="0" maxOccurs="1">
													<xsd:annotation>
														<xsd:documentation>Lämmitysjärjestelmän kuvaus suomeksi.
														</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:minLength value="1"/>
															<xsd:maxLength value="75"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="LammitysKuvaus_Sv" minOccurs="0" maxOccurs="1">
													<xsd:annotation>
														<xsd:documentation>Lämmitysjärjestelmän kuvaus ruotsiksi.
														</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:minLength value="1"/>
															<xsd:maxLength value="75"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="LammitysTilatJaIv" minOccurs="0" maxOccurs="1" type="LammitysjarjestelmaType">
													<xsd:annotation>
														<xsd:documentation>Tilojen ja ilmanvaihdon lämmitys
														</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="LammitysLamminKayttovesi" minOccurs="0" maxOccurs="1" type="LammitysjarjestelmaType">
													<xsd:annotation>
														<xsd:documentation>Lämpimän käyttöveden valmistus.
														</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="LammitysTakka" minOccurs="0" maxOccurs="1" type="LammitysMaaraTuottoType">
													<xsd:annotation>
														<xsd:documentation>Varaavien tulisijojen lukumäärä ja
															tuotto.</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="LammitysIlmanlampopumppu" minOccurs="0" maxOccurs="1" type="LammitysMaaraTuottoType">
													<xsd:annotation>
														<xsd:documentation>Ilmalämpöpumppujen lukumäärä ja tuotto.
														</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="LahtotiedotJaahdytysjarjestelma" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="JaahdytysjarjestelmaJaahdytyskaudenPainotettuKylmakerroin" minOccurs="0" maxOccurs="1">
													<xsd:annotation>
														<xsd:documentation>Jäähdytysjärjestelmän jäähdytyskauden
															painotettu kylmäkerroin.</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="vakiot:LengthLimitedDecimalType">
															<xsd:minInclusive value="0.0"/>
															<xsd:maxInclusive value="30.0"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="LahtotiedotLkvnKaytto" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="LkvnKayttoKulutusPerNelio" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Lämpimän käyttöveden ominaiskulutus
															(dm3/(m2*a)).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="LkvnKayttoVuosikulutus" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Lämpimän käyttöveden lämmitysenergian
															nettotarve (kWh/(m2*a)).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="LahtotiedotSisKuormat" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="SisKuorma" type="SisaisetKuormatType" minOccurs="1" maxOccurs="3"/>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Tulokset">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="TuloksetKaytettavatEnergiamuodot" minOccurs="0" maxOccurs="1">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="KaytettavaEnergiamuoto" minOccurs="1" maxOccurs="5">
													<xsd:complexType>
														<xsd:sequence>
															<xsd:element name="EnergiamuotoVakio" type="vakiot:EnergiamuotoNimiVakioType">
																<xsd:annotation>
																	<xsd:documentation>Energiamuodon vakioitu nimi. Järjestelmä täyttää energiamuodon
																		kertoimen automaattisesti.
																	</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
															<xsd:element name="LaskettuOstoenergia" type="vakiot:LengthLimitedDecimalType">
																<xsd:annotation>
																	<xsd:documentation>Lasketun ostoenergia määrä (kWh/a)
																	</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
														</xsd:sequence>
													</xsd:complexType>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="TuloksetUusiutuvatOmavaraisenergiat" minOccurs="0" maxOccurs="1">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="UusiutuvaOmavaraisenergia" minOccurs="1" maxOccurs="6">
													<xsd:complexType>
														<xsd:sequence>
															<xsd:element name="NimiVakio" type="vakiot:UusiutuvaOmavaraisenergiaNimiVakioType">
																<xsd:annotation>
																	<xsd:documentation>Uusiutuvan omavaraisenergian vakioitu nimi.
																	</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
															<xsd:element name="Vuosikulutus" type="vakiot:PosDecimalType">
																<xsd:annotation>
																	<xsd:documentation>Uusiutuvan omavaraisenergian
																		vuosikulutus.</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
														</xsd:sequence>
													</xsd:complexType>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="TuloksetTeknisetJarjestelmat" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="TjTilojenLammitys" minOccurs="0" maxOccurs="1" type="LammitysSahkoLampoType">
													<xsd:annotation>
														<xsd:documentation>Tilojen lämmitykseen käytetty energia.
														</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="TjTuloilmanLammitys" minOccurs="0" maxOccurs="1" type="LammitysSahkoLampoType">
													<xsd:annotation>
														<xsd:documentation>Tuloilman lämmitykseen käytetty
															energia.</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="TjKayttovedenValmistus" minOccurs="0" maxOccurs="1" type="LammitysSahkoLampoJaahdytysType">
													<xsd:annotation>
														<xsd:documentation>Käyttöveden valmistukseen käytetty
															energia (kWh/(m2*a)).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="TjIvSahko" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Ilmanvaihtojärjestelmän
															sähköenergiankulutus (kWh/(m2*a)).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="TjJaahdytys" minOccurs="0" maxOccurs="1" type="LammitysSahkoLampoJaahdytysType">
													<xsd:annotation>
														<xsd:documentation>Jäähdytysjärjestelmän energiankulutus
															(kWh/(m2*a)).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="TjKuluttajalaitteetJaValaistusSahko" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Kuluttajalaitteiden ja valaistuksen
															sähköenergiankulutus (kWh/(m2*a)).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="TuloksetNettotarve" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="NtTilojenLammitysVuosikulutus" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Tilojen lämmityksen energian nettotarve
															(kWh/a).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="NtIlmanvaihdonLammitysVuosikulutus" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Ilmanvaihdon lämmitysenergian
															nettotarve (kWh/a).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="NtKayttovedenValmistusVuosikulutus" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Käyttöveden valmistuksen energian
															nettotarve (kWh/a).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="NtJaahdytysVuosikulutus" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Jäähdytysenergian nettotarve (kWh/a).
														</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="TuloksetLampokuormat" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="LkAurinko" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Auringon tuottama lämpöenergia (kWh/a).
														</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="LkIhmiset" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Ihmisten tuottama lämpöenergia (kWh/a).
														</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="LkKuluttajalaitteet" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Kuluttajalaitteiden tuottama
															lämpöenergia (kWh/a).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="LkValaistus" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Valaistuksen tuottama lämpöenergia
															(kWh/a).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="LkKvesi" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Lämpimän käyttöveden kierrosta ja
															varastoinnin häviöistä saatu lämpöenergia (kWh/a).
														</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
