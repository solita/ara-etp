<?xml version="1.0" encoding="UTF-8"?>
<!-- Published by JAX-WS RI (http://jax-ws.java.net). RI's version is JAX-WS RI 2.3.0 svn-revision#3528ea595bd29309f69172d231bbce272d21035c. -->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.energiatodistusrekisteri.fi/ws/energiatodistustypes-2018" xmlns:vakiot="http://www.energiatodistusrekisteri.fi/ws/energiatodistustypes-2018/vakiot" targetNamespace="http://www.energiatodistusrekisteri.fi/ws/energiatodistustypes-2018" elementFormDefault="qualified">
	<xsd:import namespace="http://www.energiatodistusrekisteri.fi/ws/energiatodistustypes-2018/vakiot" schemaLocation="http://localhost:8080/etp/ws/etpservice?xsd=3"/>
	<xsd:element name="EnergiatodistusIlmoitus" type="EnergiatodistusRequestType"/>
	<xsd:element name="EnergiatodistusVastaus" type="EnergiatodistusResponseType"/>
	<xsd:complexType name="EnergiatodistusResponseType">
		<xsd:sequence>
			<xsd:choice>
				<xsd:element name="TodistusTunnus" type="xsd:int">
					<xsd:annotation>
						<xsd:documentation>Tallennetun todistuksen tunnistenumero. Elementtiä jää puuttumaan, 
						jos tallennus epäonnistuu esimerkiksi tarkistusten vuoksi.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>					
				<xsd:element name="Virhe" type="xsd:string" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Jos tallennus epäonnistuu, Virhe-elementeissä kuvaillaan 
						kaikki energiatodistuksen tarkisteissa havaitut puutteet. Osa niistä ei välttämättä
						yksinään riitä hylkäämään todistusta.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:element name="Huomautus" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Jos tallennus on onnistunut onnistuu, Huomautus-elementeissä palautetaan 
					energiatodistuksen tarkisteiden havainnot. Huomautukset eivät estä energiatodistuksen
					tallentamista.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EnergiatodistusRequestType">
		<xsd:sequence>
			<xsd:element name="Energiatodistus">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Perustiedot">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="Nimi" minOccurs="0" maxOccurs="1">
										<xsd:annotation>
											<xsd:documentation>Rakennuksen nimi</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:minLength value="1"/>
												<xsd:maxLength value="50"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="Rakennustunnus" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Pysyvä rakennustunnus. 10 merkkiä pitkä, sisältää 9 numeerista merkkiä.
												Alkaa aina 1:llä. Tunnuksen lopussa aakkosnumeerinen (kirjain tai numero) tarkistemerkki. Esim. 100012345N
												Pakollinen jos Laatimisvaihe = 1 tai 2.
											</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:minLength value="10"/>
												<xsd:maxLength value="10"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="Kiinteistotunnus" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Kiinteistötunnus.
												Pakollinen jos Laatimisvaihe = 0.</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:minLength value="1"/>
												<xsd:maxLength value="50"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="Katuosoite_Fi" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Rakennuksen katuosoite suomeksi</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:minLength value="1"/>
												<xsd:maxLength value="100"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="Katuosoite_Sv" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Rakennuksen katuosoite ruotsiksi</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:minLength value="1"/>
												<xsd:maxLength value="100"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="Postinumero" type="vakiot:PostinumeroType">
										<xsd:annotation>
											<xsd:documentation>Rakennuksen postinumero
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="OnkoJulkinenRakennus" minOccurs="1" maxOccurs="1" type="xsd:boolean">
										<xsd:annotation>
											<xsd:documentation>Kertoo, onko rakennus julkinen. false=ei,
												true=kyllä</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="Laatimisvaihe" minOccurs="1" maxOccurs="1">
										<xsd:annotation>
											<xsd:documentation>Missä vaiheessa energiatodistus on laadittu? 0 = Rakennuslupaa haettaessa, 1 = Käyttöönottovaiheessa, 2 = Olemassa olevalle rakennukselle.
											</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:integer">
												<xsd:minInclusive value="0"/>
												<xsd:maxInclusive value="2"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="Havainnointikaynti" minOccurs="0" maxOccurs="1" type="xsd:date">
										<xsd:annotation>
											<xsd:documentation>Päivämäärä jolloin havainnointikäynti on suoritettu. Pakollinen jos Laatimisvaihe = 2.
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="Rakennusosa" minOccurs="0" maxOccurs="1">
										<xsd:annotation>
											<xsd:documentation>Rakennuksen osan nimi.</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:minLength value="1"/>
												<xsd:maxLength value="100"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="Valmistumisvuosi" type="xsd:gYear">
										<xsd:annotation>
											<xsd:documentation>Valmistumisvuosi</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="Kayttotarkoitus" minOccurs="1" maxOccurs="1" type="vakiot:KayttotarkoitusType">
										<xsd:annotation>
											<xsd:documentation>Käyttötarkoitusluokka, viittaus
												kayttotarkoitusluokkakoodistoon.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="Kieli" minOccurs="0" maxOccurs="1">
										<xsd:annotation>
											<xsd:documentation>
												Energiatodistuksen kieli. 0 = Suomi, 1 = Ruotsi, 2 = kaksikielinen.
												Jos uupuu niin ei määritelty.
											</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:integer">
												<xsd:minInclusive value="0"/>
												<xsd:maxInclusive value="2"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="Tilaaja" minOccurs="0" maxOccurs="1">
										<xsd:annotation>
											<xsd:documentation>Energiatodistuksen tilaaja
											</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:minLength value="1"/>
												<xsd:maxLength value="200"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="Yritys" minOccurs="0" maxOccurs="1">
										<xsd:annotation>
											<xsd:documentation> Todistuskohtainen yritys
											</xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="Nimi">
													<xsd:annotation>
														<xsd:documentation>Todistuskohtaisen yrityksen nimi
														</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:minLength value="1"/>
															<xsd:maxLength value="150"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="Katuosoite" minOccurs="0" maxOccurs="1">
													<xsd:annotation>
														<xsd:documentation>Todistuskohtaisen yrityksen katuosoite
														</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:minLength value="1"/>
															<xsd:maxLength value="100"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="Postinumero" minOccurs="0" maxOccurs="1">
													<xsd:annotation>
														<xsd:documentation>Todistuskohtaisen yrityksen postinumero
														</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:minLength value="1"/>
															<xsd:maxLength value="8"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="Postitoimipaikka" minOccurs="0" maxOccurs="1">
													<xsd:annotation>
														<xsd:documentation>
														Todistuskohtaisen yrityksen postitoimipaikka. 
														Mikäli postinumero on suomalainen, postitoimipaikkaa ei tarvitse antaa.
														</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:minLength value="1"/>
															<xsd:maxLength value="30"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="KeskeisetSuositukset_Fi" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												Keskeiset suositukset rakennuksen energiatehokkuutta parantaviksi
												toimenpiteiksi suomeksi. Osio ei koske uudisrakennuksia, mutta
												pakollinen vanhoille rakennuksille.
											</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="2500"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="KeskeisetSuositukset_Sv" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												Keskeiset suositukset rakennuksen energiatehokkuutta parantaviksi
												toimenpiteiksi ruotsiksi. Osio ei koske uudisrakennuksia, mutta
												pakollinen vanhoille rakennuksille.
											</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="2500"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Lahtotiedot">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="LammitettyNettoala" minOccurs="1" maxOccurs="1" type="vakiot:PosDecimalType">
										<xsd:annotation>
											<xsd:documentation>Rakennuksen lämmitetty nettoala (m2).
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="LahtotiedotRakennusvaippa" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="Ilmanvuotoluku" minOccurs="0" maxOccurs="1">
													<xsd:annotation>
														<xsd:documentation>Rakennuksen ilmanvuotoluku (m3/(h*m2))
														</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="vakiot:LengthLimitedDecimalType">
															<xsd:minInclusive value="0.0"/>
															<xsd:maxInclusive value="50.0"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="RvUlkoseinat" minOccurs="0" maxOccurs="1">
													<xsd:complexType>
														<xsd:sequence>
															<xsd:element name="RvA" type="vakiot:PosDecimalType"/>
															<xsd:element name="RvU">
																<xsd:annotation>
																	<xsd:documentation>Rakennusvaipan ulkoseinien U-arvo
																		(W/(m2*K))</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="vakiot:LengthLimitedDecimalType">
																		<xsd:minInclusive value="0.05"/>
																		<xsd:maxInclusive value="2.0"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:element>
														</xsd:sequence>
													</xsd:complexType>
												</xsd:element>
												<xsd:element name="RvYlapohja" minOccurs="0" maxOccurs="1">
													<xsd:complexType>
														<xsd:sequence>
															<xsd:element name="RvA" type="vakiot:PosDecimalType"/>
															<xsd:element name="RvU">
																<xsd:annotation>
																	<xsd:documentation>Rakennusvaipan yläpohjan U-arvo
																		(W/(m2*K))</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="vakiot:LengthLimitedDecimalType">
																		<xsd:minInclusive value="0.03"/>
																		<xsd:maxInclusive value="2.0"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:element>
														</xsd:sequence>
													</xsd:complexType>
												</xsd:element>
												<xsd:element name="RvAlapohja" minOccurs="0" maxOccurs="1">
													<xsd:complexType>
														<xsd:sequence>
															<xsd:element name="RvA" type="vakiot:PosDecimalType"/>
															<xsd:element name="RvU">
																<xsd:annotation>
																	<xsd:documentation>Rakennusvaipan alapohjan U-arvo
																		(W/(m2*K))</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="vakiot:LengthLimitedDecimalType">
																		<xsd:minInclusive value="0.03"/>
																		<xsd:maxInclusive value="4.0"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:element>
														</xsd:sequence>
													</xsd:complexType>
												</xsd:element>
												<xsd:element name="RvIkkunat" minOccurs="0" maxOccurs="1">
													<xsd:complexType>
														<xsd:sequence>
															<xsd:element name="RvA" type="vakiot:PosDecimalType"/>
															<xsd:element name="RvU">
																<xsd:annotation>
																	<xsd:documentation>Rakennusvaipan ikkunoiden U-arvo
																		(W/(m2*K))</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="vakiot:LengthLimitedDecimalType">
																		<xsd:minInclusive value="0.4"/>
																		<xsd:maxInclusive value="6.5"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:element>
														</xsd:sequence>
													</xsd:complexType>
												</xsd:element>
												<xsd:element name="RvUlkoovet" minOccurs="0" maxOccurs="1">
													<xsd:complexType>
														<xsd:sequence>
															<xsd:element name="RvA" type="vakiot:PosDecimalType"/>
															<xsd:element name="RvU">
																<xsd:annotation>
																	<xsd:documentation>Rakennusvaipan ulko-ovien U-arvo
																		(W/(m2*K))</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="vakiot:LengthLimitedDecimalType">
																		<xsd:minInclusive value="0.2"/>
																		<xsd:maxInclusive value="6.5"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:element>
														</xsd:sequence>
													</xsd:complexType>
												</xsd:element>
												<xsd:element name="RvKylmasillatUA" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Rakennusvaipan kylmäsiltojen pinta-alan
															ja U-arvon tulo.</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="LahtotiedotIkkunat" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="Pohjoinen" minOccurs="0" maxOccurs="1" type="IkkunaType">
													<xsd:annotation>
														<xsd:documentation>Pohjoisikkunoiden tiedot.
														</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="Koillinen" minOccurs="0" maxOccurs="1" type="IkkunaType">
													<xsd:annotation>
														<xsd:documentation>Koillisikkunoiden tiedot.
														</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="Ita" minOccurs="0" maxOccurs="1" type="IkkunaType">
													<xsd:annotation>
														<xsd:documentation>Itäikkunoiden tiedot.
														</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="Kaakko" minOccurs="0" maxOccurs="1" type="IkkunaType">
													<xsd:annotation>
														<xsd:documentation>Kaakkoisikkunoiden tiedot.
														</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="Etela" minOccurs="0" maxOccurs="1" type="IkkunaType">
													<xsd:annotation>
														<xsd:documentation>Eteläikkunoiden tiedot.
														</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="Lounas" minOccurs="0" maxOccurs="1" type="IkkunaType">
													<xsd:annotation>
														<xsd:documentation>Lounasikkunoiden tiedot.
														</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="Lansi" minOccurs="0" maxOccurs="1" type="IkkunaType">
													<xsd:annotation>
														<xsd:documentation>Länsi-ikkunoiden tiedot.
														</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="Luode" minOccurs="0" maxOccurs="1" type="IkkunaType">
													<xsd:annotation>
														<xsd:documentation>Luoteisikkunoiden tiedot.
														</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="LahtotiedotIlmanvaihto" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="IvKuvaus_Fi" minOccurs="0" maxOccurs="1">
													<xsd:annotation>
														<xsd:documentation>Ilmanvaihtojärjestelmän kuvaus suomeksi.
														</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:minLength value="1"/>
															<xsd:maxLength value="75"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="IvKuvaus_Sv" minOccurs="0" maxOccurs="1">
													<xsd:annotation>
														<xsd:documentation>Ilmanvaihtojärjestelmän kuvaus ruotsiksi.
														</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:minLength value="1"/>
															<xsd:maxLength value="75"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="IvPaaivTulo" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Pääilmanvaihtokoneen tuloilmavirta
															(m3/s).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="IvPaaivPoisto" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Pääilmanvaihtokoneen poistoilmavirta
															(m3/s).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="IvPaaivSfp" minOccurs="0" maxOccurs="1">
													<xsd:annotation>
														<xsd:documentation>Pääilmanvaihtokoneen ilmanvaihdon
															ominaissähköteho (kW/(m3*s)).</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="vakiot:PosDecimalType">
															<xsd:maxInclusive value="10.0"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="IvPaaivLampotilasuhde" minOccurs="0" maxOccurs="1">
													<xsd:annotation>
														<xsd:documentation>Pääilmanvaihtokoneen lämmöntalteenoton
															lämpötilasuhde.</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="vakiot:PosDecimalType">
															<xsd:maxInclusive value="1.0"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="IvPaaivJaatymisenesto" minOccurs="0" maxOccurs="1">
