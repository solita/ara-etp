										<xsd:annotation>
											<xsd:documentation>
												Laskentatyökalun nimi ja versionumero.
											</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:minLength value="1"/>
												<xsd:maxLength value="60"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="ToteutunutOstoenergiankulutus" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="OstettuEnergia" minOccurs="0" maxOccurs="1">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="ToKaukolampoVuosikulutus" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Ostettu kaukolämpöenergia vuodessa
															(kWh/a).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="ToKokonaissahkoVuosikulutus" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Ostettu kokonaissähköenergia
															vuodessa(kWh/a).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="ToKiinteistosahkoVuosikulutus" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Ostettu kiinteistösähköenergia
															vuodessa(kWh/a).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="ToKayttajasahkoVuosikulutus" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Ostettu käyttäjäsähköenergia
															vuodessa(kWh/a).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="ToKaukojaahdytysVuosikulutus" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Ostettu kaukojäähdytysenergia
															vuodessa(kWh/a).</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="OstetutPolttoaineet" minOccurs="0" maxOccurs="1">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="OpKevytPolttooljy" minOccurs="0" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Kevyen polttoöljyn vuosikulutus
															litroina</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="OpPilkkeetHavuSekapuu" minOccurs="0" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Havu- ja sekapilkkeiden vuosikulutus
															pinokuutioina.</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="OpPilkkeetKoivu" minOccurs="0" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Koivupilkkeiden vuosikulutus
															pinokuutioina.</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="OpPuupelletit" minOccurs="0" type="vakiot:PosDecimalType">
													<xsd:annotation>
														<xsd:documentation>Puupellettien vuosikulutus
															kilogrammoina.</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="OpVapaa" minOccurs="0" maxOccurs="4">
													<xsd:complexType>
														<xsd:sequence>
															<xsd:element name="OpNimi">
																<xsd:annotation>
																	<xsd:documentation>Polttoaineen vapaamuotoinen nimi
																	</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:minLength value="1"/>
																		<xsd:maxLength value="30"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:element>
															<xsd:element name="OpYksikko">
																<xsd:annotation>
																	<xsd:documentation>Polttoaineen vapaamuotoinen yksikkö
																	</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:minLength value="1"/>
																		<xsd:maxLength value="12"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:element>
															<xsd:element name="OpMuunnoskerroin" type="vakiot:PosDecimalType">
																<xsd:annotation>
																	<xsd:documentation>Polttoaineen yksikön muunnoskerroin
																		energiaksi.</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
															<xsd:element name="OpMaaraVuodessa" type="vakiot:PosDecimalType">
																<xsd:annotation>
																	<xsd:documentation>Polttoaineen kulutus vuodessa
																		ilmaistuna OpYksikko-elementissä käytetyssä
																		mittayksikössä.</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
														</xsd:sequence>
													</xsd:complexType>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="ToSahkoVuosikulutusYhteensa" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
										<xsd:annotation>
											<xsd:documentation>Toteutunut ostettu sähköenergia vuodessa
												(kWh/a).</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ToKaukolampoVuosikulutusYhteensa" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
										<xsd:annotation>
											<xsd:documentation>Toteutunut ostettu kaukolämmön kulutus
												vuodessa (kWh/a).</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ToPolttoaineetVuosikulutusYhteensa" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
										<xsd:annotation>
											<xsd:documentation>Toteutunut ostettujen polttoaineiden määrä
												vuodessa (kWh/a).</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ToKaukojaahdytysVuosikulutusYhteensa" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
										<xsd:annotation>
											<xsd:documentation>Toteutunut ostettu kaukojäähdytysenergian
												kulutus vuodessa (kWh/a).</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Huomiot" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="HuomiotYmparys" minOccurs="0" maxOccurs="1" type="HuomioToimenpideEhdotusType">
										<xsd:annotation>
											<xsd:documentation>Huomiot - ulkoseinät, ulko-ovet ja ikkunat
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="HuomiotAlapohjaYlapohja" minOccurs="0" maxOccurs="1" type="HuomioToimenpideEhdotusType">
										<xsd:annotation>
											<xsd:documentation>Huomiot ylä- ja alapohja
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="HuomiotLammitys" minOccurs="0" maxOccurs="1" type="HuomioToimenpideEhdotusType">
										<xsd:annotation>
											<xsd:documentation>Huomiot - tilojen ja käyttöveden
												lämmitysjärjestelmät</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="HuomiotIvIlmastointi" minOccurs="0" maxOccurs="1" type="HuomioToimenpideEhdotusType">
										<xsd:annotation>
											<xsd:documentation>Huomiot - ilmanvaihto- ja
												ilmastointijärjestelmät</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="HuomiotValaistusMuut" minOccurs="0" maxOccurs="1" type="HuomioToimenpideEhdotusType">
										<xsd:annotation>
											<xsd:documentation>Huomiot - valaistus,
												jäähdytysjärjestelmät, sähköiset erillislämmitykset ja muut
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="HuomiotSuositukset_Fi" minOccurs="0" maxOccurs="1" type="HuomioSuositusStringType">
										<xsd:annotation>
											<xsd:documentation>Suosituksia rakennuksen käyttöön ja
												ylläpitoon suomeksi</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="HuomiotSuositukset_Sv" minOccurs="0" maxOccurs="1" type="HuomioSuositusStringType">
										<xsd:annotation>
											<xsd:documentation>Suosituksia rakennuksen käyttöön ja
												ylläpitoon ruotsiksi</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="HuomiotLisatietoja_Fi" minOccurs="0" maxOccurs="1">
										<xsd:annotation>
											<xsd:documentation>Lisätietoja energiatehokkuudesta suomeksi
											</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:minLength value="1"/>
												<xsd:maxLength value="500"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="HuomiotLisatietoja_Sv" minOccurs="0" maxOccurs="1">
										<xsd:annotation>
											<xsd:documentation>Lisätietoja energiatehokkuudesta ruotsiksi
											</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:minLength value="1"/>
												<xsd:maxLength value="500"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Lisamerkintoja_Fi" minOccurs="0" maxOccurs="1">
							<xsd:annotation>
								<xsd:documentation>Energiatodistuksen lisämerkintöjä suomeksi.
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="1"/>
									<xsd:maxLength value="6300"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="Lisamerkintoja_Sv" minOccurs="0" maxOccurs="1">
							<xsd:annotation>
								<xsd:documentation>Energiatodistuksen lisämerkintöjä ruotsiksi.
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="1"/>
									<xsd:maxLength value="6300"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="IkkunaType">
		<xsd:sequence>
			<xsd:element name="IkkA" minOccurs="1" maxOccurs="1" type="vakiot:PosDecimalType">
				<xsd:annotation>
					<xsd:documentation>Ikkunoiden pinta-ala (m2).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IkkU" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Ikkunoiden U-arvo (W/(m2*K))</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="vakiot:LengthLimitedDecimalType">
						<xsd:minInclusive value="0.4"/>
						<xsd:maxInclusive value="6.5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="IkkG" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Ikkunoiden kohtisuoran auringonsäteilyn
						kokonaisläpäisykerroin (g).</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="vakiot:LengthLimitedDecimalType">
						<xsd:minInclusive value="0.1"/>
						<xsd:maxInclusive value="1.0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="LammitysjarjestelmaType">
		<xsd:sequence>
			<xsd:element name="TuotonHyotysuhde" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
				<xsd:annotation>
					<xsd:documentation>Lämmitysjärjestelmän tuoton hyötysuhde
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="JaonHyotysuhde" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
				<xsd:annotation>
					<xsd:documentation>Lämmitysjärjestelmän jaon ja luovutuksen
						hyötysuhde.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Lampokerroin" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
				<xsd:annotation>
					<xsd:documentation>Lämmitysjärjestelmän lämpökerroin.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Apulaitteet" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
				<xsd:annotation>
					<xsd:documentation>Lämmitysjärjestelmän apulaitteiden sähkönkäyttö.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="LammitysMaaraTuottoType">
		<xsd:sequence>
			<xsd:element name="Maara" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Lämmityslaitteiden lukumäärä.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="100"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Tuotto" minOccurs="1" maxOccurs="1" type="vakiot:PosDecimalType">
				<xsd:annotation>
					<xsd:documentation>Lämmityslaitteiden tuotto (kWh).
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="LammitysSahkoLampoType">
		<xsd:sequence>
			<xsd:element name="Sahko" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
				<xsd:annotation>
					<xsd:documentation>Lämmitykseen käytetty sähköenergia (kWh/(m2*a)).
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Lampo" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
				<xsd:annotation>
					<xsd:documentation>Lämmitykseen käytetty lämpöenergia (kWh/(m2*a)).
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="LammitysSahkoLampoJaahdytysType">
		<xsd:complexContent>
			<xsd:extension base="LammitysSahkoLampoType">
				<xsd:sequence>
					<xsd:element name="Kaukojaahdytys" minOccurs="0" maxOccurs="1" type="vakiot:PosDecimalType">
						<xsd:annotation>
							<xsd:documentation>Käytetty jäähdytysenergia (kWh/(m2*a)).
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:simpleType name="HuomioStringType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="1000"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="HuomioSuositusStringType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="1500"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:complexType name="SisaisetKuormatType">
		<xsd:sequence>
			<xsd:element name="Selite_Fi" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Sisäisen lämpökuorman selite suomeksi.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="35"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Selite_Sv" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Sisäisen lämpökuorman selite ruotsiksi.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="35"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Kayttoaste" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Sisäisen lämpökuorman käyttöaste.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="vakiot:LengthLimitedDecimalType">
						<xsd:minInclusive value="0.1"/>
						<xsd:maxInclusive value="1.0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Henkilot" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Sisäinen lämpökuorma: henkilöiden energiantuotto
						(W/m2).</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="vakiot:LengthLimitedDecimalType">
						<xsd:minInclusive value="0.0"/>
						<xsd:maxInclusive value="14.0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Kuluttajalaitteet" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Sisäinen lämpökuorma: kuluttajalaitteiden
						energiantuotto (W/m2).</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="vakiot:LengthLimitedDecimalType">
						<xsd:minInclusive value="0.0"/>
						<xsd:maxInclusive value="12.0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Valaistus" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Sisäinen lämpökuorma: valaistuksen
						energiantuotto (W/m2).</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="vakiot:LengthLimitedDecimalType">
						<xsd:minInclusive value="0.0"/>
						<xsd:maxInclusive value="19.0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="HuomioToimenpideEhdotusType">
		<xsd:sequence>
			<xsd:element name="HuomiotTeksti_Fi" type="HuomioStringType" minOccurs="0"/>
			<xsd:element name="HuomiotTeksti_Sv" type="HuomioStringType" minOccurs="0"/>
			<xsd:element name="Toimenpide" type="ToimenpideEhdotusType" minOccurs="0" maxOccurs="3"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="ToimenpideEhdotusType">
		<xsd:sequence>
			<xsd:element name="Nimi_Fi" minOccurs="0" maxOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="100"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<xsd:element name="Nimi_Sv" minOccurs="0" maxOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="100"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<xsd:element name="Lampo" minOccurs="0" maxOccurs="1" type="vakiot:LengthLimitedDecimalType"/>
			<xsd:element name="Sahko" minOccurs="0" maxOccurs="1" type="vakiot:LengthLimitedDecimalType"/>
			<xsd:element name="Jaahdytys" minOccurs="0" maxOccurs="1" type="vakiot:LengthLimitedDecimalType"/>
			<xsd:element name="EluvunMuutos" minOccurs="0" maxOccurs="1" type="vakiot:LengthLimitedDecimalType"/>
		</xsd:sequence>
	</xsd:complexType>

</xsd:schema>
