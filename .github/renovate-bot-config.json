{
  "branchPrefix": "renovate-fix/",
  "gitAuthor": "Renovate Bot <bot@renovateapp.com>",
  "platform": "github",
  "repositories": [
    "solita/ara-etp"
  ],
  "extends": ["config:recommended"],
  "dependencyDashboard": true,
  "labels": ["dependencies"],
  "packageRules": [
    {
      "matchPackagePatterns": ["metosin/reitit-"],
      "groupName": "Reitit dependencies"
    },
    {
      "matchPackagePatterns": ["org.flywaydb/flyway-"],
      "groupName": "Flyway dependencies"
    }
  ]
}
