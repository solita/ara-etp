{
  "plugins": ["./jsdoc/plugins/sig"],
  "recurseDepth": 10,
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "\\w+(_test|\\.stories)\\.js$"
  },
  "opts": {
    "encoding": "utf-8",
    "destination": "./jsdoc/out",
    "template": "node_modules/docdash"
  }
}
